---
import Layout from '../layouts/Layout.astro';
import SpritesBar from '../components/SpriteBar.jsx';
import WelcomeMessage from '../components/WelcomeMessage.jsx';
import "../styles/global.css";
---

<Layout>
  <main class="flex flex-col gap-20 items-center mt-10">
    <SpritesBar client:load direction="left" />
    <WelcomeMessage client:only="react" />
    <SpritesBar client:load direction="right" />
  </main>
</Layout>
